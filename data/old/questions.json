  "category_packs": {
    "Smart Safe": [
      {
        "key": "smart_safe_additions",
        "title_key": "section.delivery",
        "fields": [
          {
            "name": "stairs_count",
            "label_key": "field.stairs_count",
            "type": "number",
            "visible_if": {
              "any": [
                {
                  "field": "loading_dock",
                  "op": "eq",
                  "value": "No"
                }
              ]
            }
          }
        ]
      }
    ],
    "Recycler": [
      {
        "key": "power_network",
        "title_key": "section.power_network",
        "fields": [
          {
            "name": "has_ethernet",
            "label_key": "field.has_ethernet",
            "type": "radio",
            "options": [
              "Yes",
              "No"
            ],
            "required": true
          },
          {
            "name": "ethernet_distance_m",
            "label_key": "field.ethernet_distance_m",
            "type": "number",
            "visible_if": {
              "all": [
                {
                  "field": "has_ethernet",
                  "op": "eq",
                  "value": "Yes"
                }
              ]
            }
          }
        ]
      }
    ],
    "Dispenser": []
  },
  "overrides": {
    "*": {
      "required": [
        "store_name"
      ],
      "defaults": {
        "loading_dock": "No"
      },
      "hide_fields": [],
      "insert_after": []
    },
    "category:Smart Safe": {
      "required": [
        "loading_dock"
      ],
      "insert_after": [
        {
          "after": "path_desc",
          "field": {
            "name": "stairs_required",
            "label_key": "field.stairs_required",
            "type": "radio",
            "options": [
              "Yes",
              "No"
            ],
            "default": "No"
          }
        }
      ]
    },
    "make:Kisan": {
      "hide_fields": [
        "stairs_required"
      ]
    },
    "model:TiDel|D4": {
      "defaults": {
        "stairs_required": "Yes"
      }
    }
  },
  "smart_safe": {
